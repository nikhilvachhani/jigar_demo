                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp"
                        android:background="@drawable/edittext_border"
                        android:padding="6dp"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/tvParticipants">

                        <androidx.appcompat.widget.AppCompatTextView
                            android:id="@+id/tvLimitedNoOfParticipants"
                            style="@style/TextViewStyleBold"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/sf_pro_medium"
                            android:gravity="start|center_vertical"
                            android:padding="10dp"
                            android:text="@string/is_limited_number_of_participants"
                            android:textColor="@color/colorSubHeaderText"
                            android:textSize="@dimen/text_size_regular"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toStartOf="@id/swLimitedNumberOfPerson"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                        <androidx.appcompat.widget.SwitchCompat
                            android:id="@+id/swLimitedNumberOfPerson"
