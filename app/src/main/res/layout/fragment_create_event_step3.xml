                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:checked="@{ceViewModel.isLimitedNumberOfParticipant}"
                            android:padding="10dp"
                            app:layout_constraintBottom_toBottomOf="@id/tvLimitedNoOfParticipants"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintTop_toTopOf="@id/tvLimitedNoOfParticipants" />

                    </androidx.constraintlayout.widget.ConstraintLayout>

                    <androidx.constraintlayout.widget.ConstraintLayout
